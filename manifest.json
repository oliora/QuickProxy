{
    "manifest_version": 2,
    "name": "Quick Proxy",
    "version": "0.1",
	"author": "Andrey Upadyshev",
    "description": "Quick switch between proxy settings",
    "icons": {
        "48": "icons/app.svg",
        "96": "icons/app.svg"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "quickproxy@oliora.name",
            "strict_min_version": "127.0"
        }
    },
    "permissions": [
        "browserSettings",
        "proxy"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icons/app.svg",
        "theme_icons": [{
            "dark": "icons/app.svg",
            "light": "icons/app-dark.svg",
            "size": 48
        }]
    }
}